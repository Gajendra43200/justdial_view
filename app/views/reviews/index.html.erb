<table>
  <thead>
    <tr>
      <th>review_id     |</th>
      <th>service_id    |</th>
       <th>user_id      |</th>
      <th>Rating        |</th>
      <th>content       |</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%@reviews.each do |review| %>
      <tr>
        <td><%= review.id %></td>
        <td><%= review.service_id%></td>
        <td><%= review.user_id%></td>
        <td><%= review.rating %></td>
        <td><%= review.content %></td>
        <td><%= link_to "Show", review %></td>
        <td><%= link_to "Edit", edit_review_path(review)%></td>
         <td><%= button_to "Destroy", review, method: :delete%></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to "New Review", new_review_path %>
<%= button_to "logout", logout_path, method: :delete%>
<%= form_with url:search_path, method: :get do |form| %>

  <%= form.label :service_name %>
  <%= form.text_field :service_name %><br>

  <%= form.label :city %>
  <%= form.text_field :city %><br>
   <%= form.submit 'Search' %>

<% end %>
